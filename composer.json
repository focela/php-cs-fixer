{
  "name": "focela/php-cs-fixer",
  "description": "Automatically configuration PHP CS Fixer for projects",
  "keywords": [
    "php",
    "php-cs-fixer",
    "focela"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Focela Technologies",
      "email": "opensource@focela.com",
      "homepage": "https://opensource.focela.com"
    }
  ],
  "require": {
    "php": "^8.0",
    "friendsofphp/php-cs-fixer": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Focela\\PhpCsFixer\\": "src"
    }
  },
  "config": {
    "optimize-autoloader": true,
    "preferred-install": "dist",
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "3.0.x-dev"
    }
  },
  "minimum-stability": "stable",
  "prefer-stable": true
}
